<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clonetrello_offlineversion">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_trello"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Card_Activity" />
        <activity android:name=".Board_Activity" />
        <activity android:name=".Main_Activity" />

        <receiver android:name=".broadcast_service.AlarmReceiver" />
    </application>

</manifest>